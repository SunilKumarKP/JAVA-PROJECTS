package com.org.programming;

public class SwapWithoutTemp {

    public static void main(String[] args) {
        int a = 20;
        int b = 50;
        System.out.printf("Before swap 'a': %d, 'b': %d %n", a, b);
        a = (a + b) - (b = a);

        System.out.printf("After swap 'a': %d, 'b': %d %n", a, b);
    }
}
