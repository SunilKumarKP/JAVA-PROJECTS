package com.org;

public class StringExample {

    public static void main(String[] args) {
        String s = "Abc";
        String s1 = new String(s);
        if (s.equals(s1)) {
            System.out.println("s equls s1");
        }
        // s1.intern();
        if (s1.equals(s.intern())) {
            System.out.println("s equls s1 after intern");
        }
    }
}
