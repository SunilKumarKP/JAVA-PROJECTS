package com.org;

public class StringExample {

    public static void main(String[] args) {
        String s = "abc";
        String s2 = "abc";
        String s1 = new String(s).intern();
        System.out.println("Are equal==" + (s == s2));

        System.out.println("Are equal==" + (s == s1));

    }
}
